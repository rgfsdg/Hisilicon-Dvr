class LG extends PtzBase{
	// 协议的显示名称, 最好与文件名直接对应，不能超过16字符
	Name = "LG"
		
	// 指明是云台协议还是矩阵协议，使用"PTZ", "MATRIX"表示
	Type = "PTZ"
	
	// 以ms为单位
	Internal = 200		
	
	// 一下地址范围如无对应的地址范围，都设成0xFF
	
	// 云台地址范围
	CamAddrRange 		= [0x00, 0xFF]
	
	// 监视地址范围
	MonAddrRange		= [0x00, 0xFF]
	
	// 预置点范围
	PresetRange 		= [0x00, 0xFF]
	
	// 自动巡航线路范围
	TourRange			= [0xFF, 0xFF]
	
	// 轨迹线路范围
	PatternRange		= [1, 3]
	
	// 垂直速度范围
	TileSpeedRange 		= [0x00, 0x40]
	
	// 水平速度范围
	PanSpeedRange 		= [0x00, 0x40]
	
	// 辅助范围
	AuxRange 			= [0x01, 0x08]
	
	AddrPos 			= 1
	PresetPos 		= 4
	TileSpeedPos 	= 6
	PanSpeedPos 	= 5
	PatternPos 		= 5
	AuxPos 			= 5
	
	cmd = []
	
	StartOpr = {
		TileUp 		= [0xE5, 0x00, 0x02, 0x00, 0x20, 0x00, 0x00, 0x00],
		TileDown 	= [0xE5, 0x00, 0x02, 0x00, 0x10, 0x00, 0x00, 0x00],
		PanLeft 	= [0xE5, 0x00, 0x02, 0x00, 0x80, 0x00, 0x00, 0x00],
		PanRight 	= [0xE5, 0x00, 0x02, 0x00, 0x40, 0x00, 0x00, 0x00],
		
		ZoomWide 	= [0xE5, 0x00, 0x02, 0x00, 0x40, 0x00, 0x00, 0x00],
		ZoomTele 	= [0xE5, 0x00, 0x02, 0x00, 0x80, 0x00, 0x00, 0x00],
		FocusNear	= [0xE5, 0x00, 0x02, 0x00, 0x20, 0x00, 0x00, 0x00],
		FocusFar 	= [0xE5, 0x00, 0x02, 0x00, 0x10, 0x00, 0x00, 0x00],
		IrisSmall	= [0xE5, 0x00, 0x02, 0x02, 0x00, 0x00, 0x00, 0x00],
		IrisLarge	= [0xE5, 0x00, 0x02, 0x04, 0x00, 0x00, 0x00, 0x00],	
		
		SetPreset 	= [0xE5, 0x00, 0x02, 0xa1, 0x00, 0x7f, 0x01, 0x00],
		GoToPreset 	= [0xE5, 0x00, 0x02, 0xa4, 0x00, 0x00, 0x00, 0x00]
	}

	StopOpr = {
		TileUp 		= [0xE5, 0x00, 0x02, 0x00, 0x20, 0x00, 0x00, 0xFF],
		TileDown 	= [0xE5, 0x00, 0x02, 0x00, 0x10, 0x00, 0x00, 0xFF],
		PanLeft 	= [0xE5, 0x00, 0x02, 0x00, 0x80, 0x00, 0x00, 0xFF],
		PanRight 	= [0xE5, 0x00, 0x02, 0x00, 0x40, 0x00, 0x00, 0xFF],
		
		ZoomWide 	= [0xE5, 0x00, 0x02, 0x00, 0x40, 0x00, 0x00, 0xFF],
		ZoomTele 	= [0xE5, 0x00, 0x02, 0x00, 0x80, 0x00, 0x00, 0xFF],
		FocusNear	= [0xE5, 0x00, 0x02, 0x00, 0x20, 0x00, 0x00, 0xFF],
		FocusFar 	= [0xE5, 0x00, 0x02, 0x00, 0x10, 0x00, 0x00, 0xFF],
		IrisSmall	= [0xE5, 0x00, 0x02, 0x02, 0x00, 0x00, 0x00, 0xFF],
		IrisLarge	= [0xE5, 0x00, 0x02, 0x04, 0x00, 0x00, 0x00, 0xFF]
	}

	// 这里有点取巧，根据预先设置的校验和的不同，来判断该命令是开始命令还是结束命令，如果是开始命令，则设置速度

	function setSpeed(ver, hor)
	{
		if (cmd[7] == 0x0)
		{
			cmd[TileSpeedPos] = ver;
			cmd[PanSpeedPos] = hor;
		}
	}

	function CheckSum()
	{
		cmd[7] = (cmd[0] + cmd[1] + cmd[2] + cmd[3] + cmd[4] + cmd[5] + cmd[6]) % 256;
	}
}

local cLG = LG();

return cLG;