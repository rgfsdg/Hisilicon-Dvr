class Yann extends PtzBase{
	// 协议的显示名称, 最好与文件名直接对应，不能超过16字符
	Name = "YAAN"
		
	// 指明是云台协议还是矩阵协议，使用"PTZ", "MATRIX"表示
	Type = "PTZ"
	
	// 以ms为单位
	Internal = 200		
	
	// 一下地址范围如无对应的地址范围，都设成0xFF
	
	// 云台地址范围
	CamAddrRange 		= [0x00, 0xFF]
	
	// 监视地址范围
	MonAddrRange		= [0x00, 0xFF]
	
	// 预置点范围
	PresetRange 		= [0x00, 0xFF]
	
	// 自动巡航线路范围
	TourRange			= [0x01, 0x01]
	
	// 轨迹线路范围
	PatternRange		= [0x01, 0xFF]
	
	// 垂直速度范围
	TileSpeedRange 		= [0x01, 0xFFF]
	
	// 水平速度范围
	PanSpeedRange 		= [0x01, 0xFF]
	
	// 辅助范围
	AuxRange 			= [0x01, 0x08]
	
	AddrPos 			= 1
	PresetPos 		= 3
	TileSpeedPos 	= 10
	PanSpeedPos 	= 9
	AuxPos 				= 5
	
	cmd = []
	
	StartOpr = {
		TileUp 		= [0x02, 0x00, 0x01, 0x04, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		TileDown 	= [0x02, 0x00, 0x01, 0x08, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		PanLeft 	= [0x02, 0x00, 0x01, 0x02, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		PanRight 	= [0x02, 0x00, 0x01, 0x01, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		LeftUp 		= [0x02, 0x00, 0x01, 0x06, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		LeftDown 	= [0x02, 0x00, 0x01, 0x0a, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		RightUp		= [0x02, 0x00, 0x01, 0x05, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		RightDown = [0x02, 0x00, 0x01, 0x09, 0x00, 0x00, 0x02, 0x01, 0x0e, 0x00, 0x00, 0x00],
		
		ZoomWide 	= [0x02, 0x00, 0x01, 0x10, 0x00, 0x00],
		ZoomTele 	= [0x02, 0x00, 0x01, 0x20, 0x00, 0x00],
		FocusNear	= [0x02, 0x00, 0x01, 0x40, 0x00, 0x00],
		FocusFar 	= [0x02, 0x00, 0x01, 0x80, 0x00, 0x00],
		IrisSmall	= [0x02, 0x00, 0x01, 0x00, 0x02, 0x00],
		IrisLarge	= [0x02, 0x00, 0x01, 0x00, 0x01, 0x00],	
		
		SetPreset 	= [0x02, 0x00, 0x05, 0x01, 0x00],
		GoToPreset 	= [0x02, 0x00, 0x02, 0x01, 0x00],
		
		SetLeftLimit 	= [0x02, 0x00, 0x0f, 0x02, 0x01, 0x00],
		SetRightLimit	= [0x02, 0x00, 0x0f, 0x02, 0x02, 0x00], 
		AutoScanOn 		= [0x02, 0x00, 0x0f, 0x03, 0x00, 0x00],

		AddTour 		= [0x02, 0x00, 0x06, 0x01, 0x00],
		DeleteTour 	= [0x02, 0x00, 0x08, 0x00, 0x00],
		StartTour 	= [0x02, 0x00, 0x03, 0x00, 0x00]
	}
	
	StopOpr = {
		TileUp 		= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		TileDown 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		PanLeft 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		PanRight 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		LeftUp 		= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		LeftDown 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		RightUp		= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		RightDown = [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		
		ZoomWide 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		ZoomTele 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		FocusNear = [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		FocusFar 	= [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		IrisSmall = [0x02, 0x00, 0x01, 0x00, 0x00, 0x00],
		IrisLarge = [0x02, 0x00, 0x01, 0x00, 0x00, 0x00]
	}
	
	function CheckSum()
	{
		if (cmd.len() == 5)
		{
			cmd[4] = (cmd[0] + cmd[1] + cmd[2] + cmd[3]) % 256;
		}
		else if (cmd.len() == 6)
		{
			cmd[5] = (cmd[0] + cmd[1] + cmd[2] + cmd[3]+ cmd[4]) % 256;
		}
		else if (cmd.len() == 12)
		{
			cmd[5] = (cmd[0] + cmd[1] + cmd[2] + cmd[3]+ cmd[4]) % 256;
			cmd[7] = cmd[1];
			cmd[11] = (cmd[6] + cmd[7] + cmd[8] + cmd[9]+ cmd[10]) % 256;
		}
	}
	
	function setSpeed(ver, hor)
  {
  	if (cmd.len() == 12)
	  {
	  	cmd[TileSpeedPos] = ver;
	  	cmd[PanSpeedPos] = hor;
	  }
  }
}

local cYann = Yann();

return cYann;