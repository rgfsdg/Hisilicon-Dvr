class PadBase {
	constructor()
	{
	}
	
	Name = "NONE"
	
	// 指明是串口协议
	Type = "PAD"
	
	// 单个命令的长度
	CommandLen = 0
	
	// 命令头长度
	HeadLen = 0
	
	// 命令头数据
	HeadData = [0]
	
	//shift键
	FuncStatus = true;
	
	// 是否通过脚本解析
//	ParseMode = 1
	
	function getCmdLen()
	{
		//::print("getCmdLen\n");
		return CommandLen;
	}
	
	function getHeadLen()
	{
		//::print("getHeadLen\n");
		return HeadLen;
	}
	
	function getHead()
	{
		//::print("getHead\n");
		return HeadData;
	}
	
	function setFuncStatus(status)
	{
	}
	
	function ParseData(cmdBuf)
	{
	}
}